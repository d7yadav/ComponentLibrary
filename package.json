{
  "name": "@dilip-design/mui-components",
  "version": "1.0.0",
  "description": "AI-friendly Material-UI v7 RC component library with comprehensive agentic coding support",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "import": "./dist/theme.mjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs"
    }
  },
  "files": [
    "dist",
    "src",
    "docs",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.17.0",
    "yarn": ">=4.0.0"
  },
  "packageManager": "yarn@4.2.2",
  "scripts": {
    "setup": "yarn setup:git && yarn setup:ai && yarn install",
    "setup:git": "git config --local user.name 'Dilip Yadav' && git config --local user.email 'dilip.sm.yadav@gmail.com'",
    "setup:ai": "node scripts/setup-ai-environment.js",
    "dev": "concurrently \"yarn dev:vite\" \"yarn dev:storybook\" \"yarn ai:monitor\"",
    "dev:vite": "vite --port 3001 --host",
    "dev:storybook": "storybook dev -p 6006 --no-open",
    "ai:monitor": "node scripts/ai-monitor-enhanced.cjs",
    "ai:monitor:fast": "node scripts/ai-monitor-enhanced.cjs --fast",
    "ai:monitor:performance": "node scripts/ai-monitor-enhanced.cjs --performance",
    "ai:monitor:visual": "node scripts/ai-monitor-enhanced.cjs --visual",
    "ai:validate": "node scripts/ai-code-validator-enhanced.cjs",
    "ai:validate:storybook": "node scripts/ai-storybook-validator.cjs",
    "ai:validate:coverage": "node scripts/ai-story-coverage-validator.cjs",
    "ai:validate:tests": "node scripts/ai-test-coverage-validator.cjs",
    "ai:validate:ux": "node scripts/ai-ux-pattern-validator.cjs",
    "ai:validate:themes": "yarn visual:analyze && yarn ai:validate:ux",
    "ai:validate:runtime": "node scripts/ai-runtime-validator.cjs",
    "ai:validate:stories": "node scripts/ai-runtime-validator.cjs --stories-only",
    "ai:validate:imports": "node scripts/ai-runtime-validator.cjs --imports-only",
    "ai:validate:css": "node scripts/ai-runtime-validator.cjs --css-only",
    "ai:fix:stories": "node scripts/ai-story-fixer.cjs",
    "ai:fix:validate": "node scripts/ai-story-fixer.cjs && npm run ai:validate:runtime",
    "ai:enhance": "node scripts/ai-component-enhancer.cjs",
    "ai:docs": "node scripts/generate-ai-docs.cjs",
    "ai:senior-engineer": "node scripts/ai-senior-engineer.cjs",
    "ai:senior-engineer:task": "node scripts/ai-senior-engineer.cjs --task",
    "ai:senior-engineer:decision": "node scripts/ai-senior-engineer.cjs --decision",
    "ai:senior-engineer:quick": "node scripts/ai-senior-engineer.cjs --quick",
    "ai:overthink": "node scripts/ai-overthinking-mode.js",
    "ai:workflow": "node scripts/unified-ai-workflow.cjs",
    "ai:review": "node scripts/ai-code-reviewer.js",
    "ai:test:auto": "node scripts/ai-test-automation.cjs",
    "ai:test:smart": "node scripts/ai-test-automation.cjs --smart",
    "ai:test:generate": "node scripts/ai-test-automation.cjs --generate",
    "ai:test:visual": "node scripts/ai-test-automation.cjs --visual",
    "ai:workflow:full": "node scripts/unified-ai-workflow.cjs --full",
    "ai:workflow:component": "node scripts/unified-ai-workflow.cjs --component",
    "ai:workflow:testing": "node scripts/unified-ai-workflow.cjs --testing",
    "ai:workflow:quality": "node scripts/unified-ai-workflow.cjs --quality",
    "ai:workflow:senior": "yarn ai:senior-engineer && yarn ai:validate && yarn ai:enhance",
    "ai:ux:review": "node scripts/ai-ux-review-automation.cjs",
    "ai:ux:review:quick": "node scripts/ai-ux-review-automation.cjs --target=75 --iterations=3",
    "ai:ux:review:strict": "node scripts/ai-ux-review-automation.cjs --target=95 --iterations=7",
    "ai:ux:review:component": "node scripts/ai-ux-review-automation.cjs --component",
    "ai:ux:review:nofix": "node scripts/ai-ux-review-automation.cjs --no-autofix",
    "quality:dashboard": "node scripts/quality-dashboard.cjs generate",
    "quality:monitor": "node scripts/quality-dashboard.cjs monitor",
    "quality:monitor:fast": "node scripts/quality-dashboard.cjs monitor 15000",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "typecheck:watch": "yarn typecheck --watch",
    "build": "yarn clean && yarn ai:validate && yarn build:lib && yarn build:types && yarn ai:package",
    "build:lib": "vite build",
    "build:types": "tsc --project tsconfig.build.json",
    "build:storybook": "storybook build --output-dir dist-storybook",
    "ai:package": "node scripts/package-ai-context.js",
    "clean": "rimraf dist dist-storybook coverage .turbo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:single": "vitest run --reporter=verbose",
    "test:ai-generated": "vitest run --grep 'AI Generated'",
    "test:coverage:validate": "node scripts/ai-test-coverage-validator.cjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:install": "playwright install",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\" \"docs/**/*.md\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\" \"docs/**/*.md\"",
    "gen:component": "plop component --ai-enhanced",
    "gen:hook": "plop hook --ai-enhanced",
    "gen:pattern": "plop pattern",
    "gen:tests": "node scripts/ai-test-generator.cjs",
    "context:analyze": "node scripts/context-optimizer.cjs --analyze",
    "context:optimize": "node scripts/context-optimizer.cjs --optimize",
    "context:preserve": "node scripts/context-optimizer.cjs --preserve",
    "context:clean": "node scripts/context-optimizer.cjs --clean-package",
    "context:full": "node scripts/context-optimizer.cjs --full",
    "storybook": "storybook dev -p 6006",
    "chromatic": "node scripts/chromatic-ai-integration-fixed.cjs",
    "chromatic:build": "node scripts/chromatic-ai-integration-fixed.cjs --build",
    "chromatic:analyze": "node scripts/chromatic-ai-integration-fixed.cjs --analyze",
    "chromatic:upload": "node scripts/chromatic-ai-integration-fixed.cjs --upload",
    "chromatic:review": "node scripts/chromatic-ai-integration-fixed.cjs --review",
    "chromatic:graceful": "node scripts/chromatic-ai-integration-fixed.cjs --graceful",
    "chromatic:themes": "yarn chromatic --auto && yarn visual:analyze",
    "visual:generate": "node scripts/visual-baseline-manager-fixed.cjs --generate",
    "visual:generate:light": "node scripts/visual-baseline-manager-fixed.cjs --generate --theme=light",
    "visual:generate:dark": "node scripts/visual-baseline-manager-fixed.cjs --generate --theme=dark",
    "visual:generate:themes": "concurrently \"yarn visual:generate:light\" \"yarn visual:generate:dark\"",
    "visual:update": "node scripts/visual-baseline-manager-fixed.cjs --update",
    "visual:update:light": "node scripts/visual-baseline-manager-fixed.cjs --update --theme=light",
    "visual:update:dark": "node scripts/visual-baseline-manager-fixed.cjs --update --theme=dark",
    "visual:update:themes": "concurrently \"yarn visual:update:light\" \"yarn visual:update:dark\"",
    "visual:approve": "node scripts/visual-baseline-manager-fixed.cjs --approve",
    "visual:approve:light": "node scripts/visual-baseline-manager-fixed.cjs --approve --theme=light",
    "visual:approve:dark": "node scripts/visual-baseline-manager-fixed.cjs --approve --theme=dark",
    "visual:reject": "node scripts/visual-baseline-manager-fixed.cjs --reject",
    "visual:reject:light": "node scripts/visual-baseline-manager-fixed.cjs --reject --theme=light",
    "visual:reject:dark": "node scripts/visual-baseline-manager-fixed.cjs --reject --theme=dark",
    "visual:cleanup": "node scripts/visual-baseline-manager-fixed.cjs --cleanup",
    "visual:analyze": "node scripts/visual-baseline-manager-fixed.cjs --analyze",
    "visual:status": "node scripts/visual-baseline-manager-fixed.cjs",
    "ai:preview": "node scripts/ai-preview-server.cjs",
    "ai:preview:headed": "node scripts/ai-preview-server.cjs --headed",
    "ai:preview:test": "node scripts/test-ai-preview.cjs",
    "perf:benchmark": "node scripts/performance-benchmark.cjs --benchmark",
    "perf:baseline": "node scripts/performance-benchmark.cjs --baseline",
    "perf:analyze": "node scripts/performance-benchmark.cjs --analyze",
    "perf:report": "node scripts/performance-benchmark.cjs --report",
    "perf:continuous": "node scripts/performance-benchmark.cjs --continuous",
    "perf:mobile": "node scripts/performance-benchmark.cjs --benchmark --device=mobile",
    "perf:desktop": "node scripts/performance-benchmark.cjs --benchmark --device=desktop",
    "analyze:bundle": "yarn build && npx vite-bundle-analyzer dist/index.mjs",
    "analyze:deps": "madge --circular src/index.ts",
    "size-limit": "size-limit",
    "validate:exports": "node scripts/validate-exports.ts",
    "validate:ai-friendliness": "node scripts/validate-ai-friendliness.js",
    "release": "yarn build && changeset publish",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^6.1.6",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@mui/base": "^5.0.0-beta.58",
    "@mui/icons-material": "^6.1.6",
    "@mui/lab": "^6.0.0-beta.12",
    "@mui/material-nextjs": "^6.1.6",
    "@mui/system": "^6.1.6",
    "@mui/utils": "^6.1.6",
    "@mui/x-charts": "^7.18.0",
    "@mui/x-codemod": "^7.18.0",
    "@mui/x-data-grid": "^7.18.0",
    "@mui/x-data-grid-pro": "^7.18.0",
    "@mui/x-date-pickers": "^7.18.0",
    "@mui/x-tree-view": "^7.18.0",
    "@types/ws": "^8.18.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.5.4",
    "react-hook-form": "^7.53.0",
    "react-window": "^1.8.10",
    "use-immer": "^0.10.0",
    "ws": "^8.18.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@chromatic-com/storybook": "^2.0.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@emotion/babel-plugin": "^11.12.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "@playwright/test": "^1.48.0",
    "@storybook/addon-a11y": "^8.3.0",
    "@storybook/addon-actions": "^8.3.0",
    "@storybook/addon-controls": "^8.3.0",
    "@storybook/addon-docs": "^8.3.0",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/addon-measure": "^8.3.0",
    "@storybook/addon-outline": "^8.3.0",
    "@storybook/addon-viewport": "^8.3.0",
    "@storybook/blocks": "^8.3.0",
    "@storybook/manager-api": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "@storybook/react-vite": "^8.3.0",
    "@storybook/theming": "^8.3.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.0.13",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "axe-core": "^4.8.2",
    "axe-playwright": "^2.0.1",
    "chromatic": "^11.10.1",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "handlebars": "^4.7.8",
    "husky": "^9.1.6",
    "jest-axe": "^10.0.0",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "madge": "^8.0.0",
    "plop": "^4.0.1",
    "prettier": "^3.3.3",
    "puppeteer": "^23.0.0",
    "puppeteer-core": "^23.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "size-limit": "^11.1.5",
    "storybook": "^8.3.0",
    "terser": "^5.43.1",
    "turbo": "^2.1.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vite-bundle-analyzer": "^0.11.0",
    "vite-plugin-dts": "^4.2.1",
    "vitest": "^2.1.0"
  },
  "ai-metadata": {
    "mui-version": "6.1.6",
    "features": [
      "next-generation-css-vars",
      "enhanced-accessibility",
      "improved-typescript",
      "advanced-theming",
      "performance-optimizations",
      "react-19-ready",
      "dark-theme-system",
      "gradient-design-system",
      "ai-workflow-integration",
      "senior-engineer-patterns"
    ],
    "complexity-levels": [
      "simple",
      "moderate",
      "complex",
      "advanced"
    ],
    "supported-patterns": [
      "component",
      "hook",
      "utility",
      "service",
      "provider"
    ],
    "code-style": "functional-typescript-strict",
    "documentation-format": "comprehensive-markdown-with-examples",
    "testing-framework": "vitest-testing-library",
    "accessibility-level": "wcag-2.1-aa"
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "150 KB",
      "gzip": true
    },
    {
      "path": "dist/components/**/*.js",
      "limit": "10 KB",
      "gzip": true
    }
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "react",
    "component-library",
    "design-system",
    "material-ui",
    "mui-v7",
    "mui-v7-rc",
    "typescript",
    "enterprise",
    "ai-friendly",
    "agentic-coding",
    "dark-theme",
    "gradients",
    "accessibility"
  ],
  "author": "Dilip Yadav <dilip.sm.yadav@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dilip-yadav/mui-design-system.git"
  },
  "homepage": "https://dilip-design-system.vercel.app",
  "bugs": {
    "url": "https://github.com/dilip-yadav/mui-design-system/issues"
  }
}
